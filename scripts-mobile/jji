local function fancyNotify(step, total)
    local percentage = math.floor((step / total) * 100)
    local barLength = 20
    local filledLength = math.floor((step / total) * barLength)
    local bar = string.rep("â–ˆ", filledLength) .. string.rep("â–‘", barLength - filledLength)

    game.StarterGui:SetCore("SendNotification", {
        Title = "âš”ï¸ DZHUB âš”ï¸",
        Text = "ðŸ”„ Loading... " .. percentage .. "%\n[" .. bar .. "]",
        Duration = 1
    })
end

-- Example loading animation
task.spawn(function()
    local totalSteps = 15
    for i = 1, totalSteps do
        fancyNotify(i, totalSteps)
        task.wait(0.1)
    end

    -- Final notification
    game.StarterGui:SetCore("SendNotification", {
        Title = "âœ… DZHUB",
        Text = "Loading completed!",
        Duration = 2
    })
end)
task.wait(4)

