    ["Yuki (Star Rage)"] = {"Star Rage: Asteroid", "Star Rage: Oberon", "Star Rage: Mars", "Star Rage: Mercury", "Star Rage: Terra", "Star Rage: Jupiter", "Domain Expansion: Celestial Opera", "Maximum: Total Collapse"}
