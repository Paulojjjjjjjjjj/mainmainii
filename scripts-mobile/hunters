                end
            end)
        end
    end
})

FarmingManual:AddToggle("AutoReplayToggle", {
    Title = "Auto Replay Dungeon",
    Default = false,
    Callback = function(state)
        autoReplayActive = state
        if state then
            task.spawn(function()
                while autoReplayActive do
                    checkAutoReplay()
