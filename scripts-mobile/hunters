    Title = "Y Distance",
    Default = 10,
    Min = 0,
    Max = 20,
    Rounding = 1,
    Callback = function(value)
        yDistance = value
    end
})

FarmingManual:AddToggle("AutoLeaveToggle", {
    Title = "Auto Leave Dungeon",
    Default = false,
    Callback = function(state)
        autoLeaveActive = state
        if state then
            task.spawn(function()
                while autoLeaveActive do
                    checkAutoLeave()
